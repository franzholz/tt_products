{
  "name": "jambagecom/tt-products",
  "type": "typo3-cms-extension",
  "description": "Shop system extension for TYPO3",
  "keywords": [
    "TYPO3",
    "extension",
    "shop",
    "basket",
    "cart"
  ],
  "support": {
    "issues": "https://jambage.com/kontakt/forum.html"
  },
  "homepage": "https://jambage.com",
  "authors": [
    {
      "name": "Franz Holzinger",
      "role": "Developer",
      "homepage": "https://ttproducts.de"
    }
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "php": "^5.5 || ^7.2",
    "typo3/cms-core": "^7.6 || ^8,7 || ^9.5 || ^10.4",
    "jambagecom/div2007": "^1.11",
    "jambagecom/table": "^0.7",
    "jambagecom/tsparser": "^0.2"
  },
  "suggest": {
    "friendsoftypo3/func": "^9.0",
    "jambagecom/addons-em": "^0.2",
    "friendsoftypo3/typo3db-legacy": "^1",
    "jambagecom/taxajax": "^0.4"
  },
  "autoload": {
    "psr-4": {
      "JambageCom\\TtProducts\\": "Classes"
    },
    "classmap": [
      "api",
      "control",
      "eid",
      "hooks",
      "lib",
      "marker",
      "model",
      "modfunc1",
      "modfunc2",
      "pi1",
      "pi_int",
      "pi_search",
      "view"
    ]
  },
  "replace": {
    "jambagecom/tt-products": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "tt_products"
    }
  }
}
